{
    "name": "ajsmith-docker_systemd",
    "version": "0.3.0",
    "author": "Alex Smith <alex.smith@redhat.com>",
    "summary": "Configure systemd services to run Docker containers.",
    "license": "GPL-2.0",
    "source": "https://github.com/ajsmith/puppet-docker_systemd.git",
    "project_page": "https://github.com/ajsmith/puppet-docker_systemd",
    "issues_url": "https://github.com/ajsmith/puppet-docker_systemd/issues",
    "tags": ["docker", "systemd"],
    "operatingsystem_support": [
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": ["7"]
        },
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": ["7"]
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": ["20", "21", "22", "23"]
        }
    ],
    "requirements": [
        {
            "name": "pe",
            "version_requirement": ">= 3.7.0"
        },
        {
            "name": "puppet",
            "version_requirement": ">= 3.4.0"
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 4.1.0"
        }
    ]
}
